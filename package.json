{
  "name": "blogify",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "dev": "yarn nodemon --inspect dist/index.js",
    "jest": "jest -i --detectOpenHandles --forceExit",
    "test:e2e": "jest --config jest-e2e.json -i --detectOpenHandles --forceExit --runInBand",
    "test:integration": "jest --config jest-integration.json -i --detectOpenHandles --forceExit",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "nodemailer": "^6.9.9",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.14",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
